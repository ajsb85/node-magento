{
  "name": "magento-rest",
  "version": "v0.1.0",
  "description": "Magento REST API client library for node.js",
  "keywords": [
    "magento",
    "rest",
    "oauth",
    "api"
  ],
  "homepage": "https://github.com/ajsb85/node-magento-rest",
  "author": "Alexander J. Salas B. <a.salas@ieee.org>",
  "repository": {
    "type": "git",
    "url": "https://github.com/ajsb85/node-magento-rest"
  },
  "scripts": {
    "test": "npm run lint && mocha",
    "lint": "jshint test/*.js lib/*.js && echo Lint passed."
  },
  "dependencies": {
    "deep-extend": "^0.6.0",
    "request": "^2.51.0"
  },
  "devDependencies": {
    "jshint": "^2.6.0",
    "mocha": "^2.1.0",
    "nock": "^0.57.0"
  },
  "main": "./lib/magento",
  "jshintConfig": {
    "boss": true,
    "node": true,
    "strict": true,
    "newcap": false,
    "undef": true,
    "unused": true,
    "onecase": true,
    "lastsemic": true,
    "indent": 2,
    "globals": {
      "escape": true,
      "describe": true,
      "it": true,
      "before": true
    }
  }
}
